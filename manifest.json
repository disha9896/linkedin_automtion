{
  "name": "linkedin scraping",
  "description": "temp chrome extension practice",
  "version": "0.0.1.2",
  "manifest_version": 2,
  "icons": {
    "128": "light-bulb.png"
  },
  "browser_action":{
    "default_title": "linkedin automation",
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  },
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["library/source.js","library/source2.js","library/jquery.js"],
      "css": ["library/bootstrap.css"]
    }
  ],
  "permissions":[
    "tabs"
  ]     
}
